{
  "name": "livecodelab",
  "version": "0.1.0",
  "description": "A live coding environment for the browser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidedc/livecodelab/issues"
  },
  "repository": "davidedc/livecodelab",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "docs": "docco docs/**/*.md -o dist/docs",
    "docs:src": "docco src/coffee/**/*.coffee -o dist/docs/src",
    "test": "mocha-webpack --webpack-config webpack.config.js \"tests/**/*.js\"",
    "testone": "mocha-webpack --webpack-config webpack.config.js",
    "prettier": "prettier --single-quote",
    "servewiki": "tiddlywiki docs/wiki --server",
    "buildwiki": "tiddlywiki docs/wiki --output ./dist/wiki --build index"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "css-loader": "^0.28.11",
    "dentist": "1.0.3",
    "docco": "^0.7.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "file-loader": "^1.1.11",
    "loglevel": "^1.4.1",
    "mocha": "^3.1.2",
    "mocha-webpack": "^0.7.0",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.1",
    "prettier": "^1.10.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "tiddlywiki": "^5.1.14",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "4.10.2",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.4"
  }
}
